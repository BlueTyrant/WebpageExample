@using WebApplication1.Models.ViewModel;
@model IEnumerable<DashboardViewModel>


@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_mytemplates.cshtml";
}

<script src="@Url.Content("~/Content/mytemplates/")js/jquery-2.1.3.min.js"></script>

    <table id="schoolTable" class="table table-bordered table-responsive table-hover " style="width:100%">
        <thead>
            <tr>
                <th>SchoolId</th>
                <th>SchoolName</th>
                <th>Principal</th>
                <th>Telephone</th>
                <th>StaffAmount</th>
            </tr>
        <thead>
        <tbody>
            @foreach (DashboardViewModel school in Model)
            {

                <tr>
                    <td>@school.name</td>
                    <td>@school.name</td>
                    <td>@school.name</td>
                    <td>@school.name</td>
                    <td>@school.name</td>
                </tr>
            }
       </tbody>
</table>





<script>
    $(document).ready(function () {
        $('#schoolTable').DataTable();
    });
</script>




